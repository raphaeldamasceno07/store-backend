{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "services:up:dev": "docker compose -f compose.yaml up -d store",
    "services:stop:dev": "docker compose -f compose.yaml stop store",
    "services:down:dev": "docker compose -f compose.yaml down store",
    "start:dev": "pnpm run services:up:dev && tsx watch src/server.ts",
    "services:up:test": "docker compose -f compose.yaml up -d store_test",
    "services:stop:test": "docker compose -f compose.yaml stop store_test",
    "services:down:test": "docker compose -f compose.yaml down store_test",
    "pretest:e2e": "pnpm run services:up:test && dotenv -e .env.test -- npx prisma migrate deploy --schema=prisma/schema.prisma && dotenv -e .env.test -- npx prisma generate",
    "test:unit": "cross-env VITEST_PROJECT=unit vitest run",
    "test:unit:watch": "cross-env VITEST_PROJECT=unit vitest",
    "test:e2e": "cross-env NODE_ENV=test VITEST_PROJECT=e2e dotenv -e .env.test vitest run",
    "test:e2e:watch": "cross-env VITEST_PROJECT=e2e vitest",
    "build": "tsup src --out-dir build --format esm",
    "start": "node build/server.js",
    "lint:check": "prettier --check . && eslint .",
    "lint:fix": "prettier --write . && eslint . --fix"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/cors": "11.2.0",
    "@fastify/static": "9.0.0",
    "@prisma/adapter-pg": "7.2.0",
    "@prisma/client": "7.2.0",
    "bcryptjs": "^3.0.3",
    "dotenv": "17.2.3",
    "fastify": "^5.6.2",
    "open": "11.0.0",
    "pg": "8.16.3",
    "uuid": "^13.0.0",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@fastify/swagger": "9.6.1",
    "@fastify/swagger-ui": "5.2.4",
    "@types/cors": "^2.8.19",
    "@types/node": "^25.0.3",
    "@types/pg": "8.16.0",
    "@types/supertest": "6.0.3",
    "@vitest/coverage-v8": "4.0.16",
    "cross-env": "10.1.0",
    "dotenv-cli": "11.0.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^3.7.4",
    "prisma": "^7.2.0",
    "supertest": "7.2.2",
    "tsup": "^8.5.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vite-tsconfig-paths": "6.0.4",
    "vitest": "4.0.16"
  }
}
