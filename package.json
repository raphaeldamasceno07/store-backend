    "@fastify/cors": "11.2.0",
    "@fastify/static": "9.0.0",
    "@prisma/adapter-pg": "7.2.0",
    "@prisma/client": "7.2.0",
    "bcryptjs": "^3.0.3",
    "dotenv": "17.2.3",
    "fastify": "^5.6.2",
    "open": "11.0.0",
    "pg": "8.16.3",
    "uuid": "^13.0.0",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@fastify/swagger": "9.6.1",
    "@fastify/swagger-ui": "5.2.4",
    "@types/cors": "^2.8.19",
    "@types/node": "^25.0.3",
