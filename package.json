{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start:dev": "npm run services:up && tsx watch src/server.ts",
    "services:up": "docker compose -f compose.yaml up -d",
    "services:stop": "docker compose -f compose.yaml stop",
    "services:down": "docker compose -f compose.yaml down",
    "start": "node build/server.js",
    "build": "tsup src --out-dir build --format esm",
    "test": "npx vitest run",
    "test:watch": "npx vitest",
    "test:coverage": "npx vitest --coverage",
    "lint:check": "prettier --check . && eslint .",
    "lint:fix": "prettier --write . && eslint . --fix"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
